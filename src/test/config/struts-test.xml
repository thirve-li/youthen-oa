<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	
	<!-- System settings -->
	<constant name="struts.objectFactory" value="spring" />
	<constant name="struts.objectFactory.spring.autoWire" value="auto" />
	<constant name="struts.convention.package.locators.basePackage" value="jp.co.proship" />
	<constant name="struts.convention.result.path" value="/WEB-INF/jsp/" />
	<constant name="struts.custom.i18n.resources" value="app_messages" />
	<constant name="struts.ui.theme" value="ppfw" />
	<constant name="struts.multipart.maxSize" value="200000000" />
	<constant name="struts.ognl.allowStaticMethodAccess" value="true" />

 
  <constant name="struts.convention.action.disableScanning" value="true"/>
	
 
	<constant name="struts.convention.default.parent.package" value="json-default" />

	<constant name="struts.devMode" value="true" />
	<constant name="struts.configuration.xml.reload" value="true" />
	<constant name="struts.i18n.reload" value="true" />

	 
 

</struts>
